%% -------------------- Defines Colors and Logos for all --------------------
\def\beamer@fhe@style@do@common{
	\definecolor{fhblue}{RGB}{0,55,116}
	\definecolor{headergrey}{RGB}{224,224,224}
	\definecolor{footerblue}{RGB}{187,204,221}
	\colorlet{barfootercolor}{footercolor}
	\colorlet{fhgroundcolor}{fhblue}
	\colorlet{footcolor}{footerblue}
	\colorlet{headercolor}{headergrey}
}

\def\beamer@fhe@style@do{%
  \expandafter\let\expandafter\next
    \csname beamer@fhe@style@do@\beamer@fhe@style\endcsname
  \ifx\next\relax \let\next\beamer@fhe@style@do@default \fi
  \next
  \beamer@fhe@style@do@common
  \usebeamercolor[fg]{normal text}%
 
}

\beamer@fhe@style@do


% ---------- Begin Set Colors -----------------------------------------------

\if@fhe@usestyletitlecolor
\setbeamercolor{title}{fg=barfootercolor}
\setbeamercolor{subtitle}{fg=barfootercolor}
\else
\setbeamercolor{title}{fg=fhgroundcolor}
\setbeamercolor{subtitle}{fg=fhgroundcolor}
\fi
\if@fhe@usestyleframetitlecolor
\setbeamercolor{frametitle}{fg=barfootercolor}
\setbeamercolor{framesubtitle}{fg=barfootercolor}
\else
\setbeamercolor{frametitle}{fg=fhgroundcolor}
\setbeamercolor{framesubtitle}{fg=fhgroundcolor}
\fi

\if@fhe@usestyleframetitlecolor
\setbeamercolor{part title}{fg=barfootercolor}
\else
\setbeamercolor{part title}{fg=fhgroundcolor}
\fi



\setbeamercolor{title in head/foot}{fg=fhgroundcolor}
\setbeamercolor{page number in head/foot}{fg=pagecolor}
\setbeamercolor{author}{fg=fhgroundcolor}
\setbeamercolor{structure}{fg=fhgroundcolor}
\setbeamercolor{date}{fg=fhgroundcolor}
\setbeamercolor{date in head/foot}{fg=fhgroundcolor}
\setbeamercolor{itemize item}{fg=barfootercolor}
\setbeamercolor{itemize subitem}{fg=barfootercolor}
\setbeamercolor{itemize subsubitem}{fg=barfootercolor}
\setbeamercolor{item projected}{bg=barfootercolor, fg=white}
\setbeamercolor{enumerate item}{fg=barfootercolor}
\setbeamercolor{enumerate subitem}{fg=barfootercolor}
\setbeamercolor{enumerate subsubitem}{fg=barfootercolor}
\setbeamercolor{section in toc}{fg=fhgroundcolor}
\setbeamercolor{subsection in toc}{fg=fhgroundcolor}
\setbeamercolor{navigation symbols}{fg=fhgroundcolor}
\setbeamercolor{normal text}{fg=fhgroundcolor}

\setbeamercolor{block title}{use=structure,fg=white,bg=barfootercolor}
\setbeamercolor{block title alerted}{use=alerted text,fg=white,bg=alerted text.fg}
\setbeamercolor{block title example}{use=example text,fg=white,bg=example text.fg}
\setbeamercolor{block body}{parent=normal text,use=block title,bg=footerbarcolor}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!25!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!25!bg}



% ---------- End Set Colors ------------------------------------------------

\DeclareOptionBeamer{style}[default]{\def\beamer@fhe@style{#1}}
\DeclareOptionBeamer{authorfontsize}[default]{\def\beamer@fhe@authorfontsize{#1}}
\DeclareOptionBeamer{aligntitlepage}[default]{\def\beamer@fhe@aligntitlepage{#1}}

\DeclareOptionBeamer{disablebar}{
\ifnum\pdfstrcmp{#1}{true}=0
	\@fhe@disablebartrue
\else
	\@fhe@disablebarfalse
\fi
}
\DeclareOptionBeamer{fitframetitle}{
\ifnum\pdfstrcmp{#1}{true}=0
	\@fhe@fitframetitletrue
\else
	\@fhe@fitframetitlefalse
\fi
}

\DeclareOptionBeamer{disablefontbold}{
\ifnum\pdfstrcmp{#1}{true}=0
	\@fhe@disablefontboldtrue
\else
	\@fhe@disablefontboldfalse
\fi
}

\DeclareOptionBeamer{usestyleframetitlecolor}{
\ifnum\pdfstrcmp{#1}{true}=0
	\@fhe@usestyleframetitlecolortrue
\else
	\@fhe@usestyleframetitlecolorfalse
\fi
}
\DeclareOptionBeamer{usestyletitlecolor}{
\ifnum\pdfstrcmp{#1}{true}=0
	\@fhe@usestyletitlecolortrue
\else
	\@fhe@usestyletitlecolorfalse
\fi
}

\DeclareOptionBeamer{disabledateinfooter}{
\ifnum\pdfstrcmp{#1}{true}=0
	\@fhe@disabledateinfootertrue
\else
	\@fhe@disabledateinfooterfalse
\fi
}
